  "notifications",
  "automation",
  "pdf",
  "email",
  "webhook",
  "reporting",
  "workflow",
  "nodejs",
  "backend",
  "async"
]
,
  "dependencies": {
    "@bull-board/api": "^6.10.1",
    "@bull-board/express": "^6.10.1",
